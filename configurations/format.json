{
  "info": "morpher.py interface",
  "arguments": [
    {
      "type": "plain",
      "text": "python morpher.py"
    },
    {
      "type": "plain",
      "text": "--src"
    },
    {
      "id": "source",
      "type": "image",
      "semantic": "in",
      "caption": "Source"
    },
    {
      "type": "image",
      "text": "--dest"
    },
    {
      "id": "destination",
      "type": "image",
      "semantic": "in",
      "caption": "destination"
    },
    {
      "type": "plain",
      "text": "--out_frames"
    },
    {
      "id": "results",
      "type": "image_folder",
      "semantic": "out",
      "caption": "Results"
    },
    {
      "type": "plain",
      "text": "--num"
    },
    {
      "id": "num_images",
      "type": "int",
      "semantic": "in",
      "min": 1,
      "max": 30,
      "default": 10,
      "caption": "Number of images"
    },
    {
      "type": "plain",
      "text": "--alpha"
    },
    {
      "id": "keep_alpha",
      "type": "boolean",
      "semantic": "in",
      "default": true,
      "caption": "Keep alpha"
    }
  ]
}